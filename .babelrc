{
    "presets": [
        "next/babel"
    ],
    "plugins": [
        [
            "babel-plugin-import",
            {
                "libraryName": "@mui/material",
                "libraryDirectory": "",
                "camel2DashComponentName": false
            },
            "core"
        ],
        [
            "babel-plugin-import",
            {
                "libraryName": "@mui/icons-material",
                "libraryDirectory": "",
                "camel2DashComponentName": false
            },
            "icons"
        ],
        "@babel/plugin-transform-modules-commonjs"
    ],
    "targets": {
        "esmodules": false
    }
}